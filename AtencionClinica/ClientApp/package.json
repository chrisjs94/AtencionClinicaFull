{
  "name": "atencionclinica",
  "version": "0.1.0",
  "private": true,
  "homepage": "/",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.10.1",
    "bootstrap": "^4.5.3",
    "classnames": "^2.2.6",
    "devextreme": "22.2.4",
    "devextreme-aspnet-data-nojquery": "2.6.1",
    "devextreme-react": "22.2.4",
    "exceljs": "^4.2.0",
    "file-saver": "^2.0.5",
    "moment": "^2.27.0",
    "numeral": "^2.0.6",
    "photoswipe": "^4.1.3",
    "prop-types": "^15.7.2",
    "react": "^18.1.0",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^5.2.1",
    "react-input-range": "^1.3.0",
    "react-intl": "^3.1.9",
    "react-moment": "^0.9.7",
    "react-redux": "^7.2.0",
    "react-router-dom": "~5.3.0",
    "react-slick": "0.25.2",
    "react-tabs": "^3.2.0",
    "react-toastify": "^5.3.2",
    "reactstrap": "~8.10.1",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0",
    "sass": "^1.32.4",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "ajv": "^6.9.1",
    "cross-env": "^5.2.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "nan": "^2.14.1",
    "react-scripts": "^5.0.1",
    "typescript": "^3.7.5"
  },
  "eslintConfig": {
    "extends": "react-app",
    "env": {
      "browser": true,
      "es2021": true
    },
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "ecmaFeatures": {
        "modules": true,
        "jsx": true
      }
    },
    "plugins": [
      "react",
      "@typescript-eslint"
    ],
    "rules": {
      "import/prefer-default-export": 0,
      "react/prop-types": 0,
      "lit-a11y/click-events-have-key-events": 0,
      "jsx-a11y/no-static-element-interactions": 0
    }
  },
  "scripts": {
    "start": "rimraf ./build && react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env CI=true react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
